{
  "name": "db0",
  "version": "0.1.1",
  "description": "Lightweight SQL Connector",
  "repository": "unjs/db0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./connectors/*": {
      "types": "./connectors/*.d.ts",
      "import": "./connectors/*.mjs",
      "require": "./connectors/*.cjs"
    },
    "./drizzle": {
      "types": "./dist/drizzle.d.ts",
      "import": "./dist/drizzle.mjs",
      "require": "./dist/drizzle.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "connectors"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts . && prettier -c src test",
    "lint:fix": "eslint --ext .ts . --fix && prettier -w src test",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "test": "pnpm lint && vitest run --coverage",
    "db0": "pnpm jiti src/cli"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/pg": "^8.10.2",
    "@vitest/coverage-v8": "^0.33.0",
    "better-sqlite3": "^8.5.0",
    "changelogen": "^0.5.4",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.2",
    "eslint": "^8.45.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.19.1",
    "pg": "^8.11.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "better-sqlite3": "^8.4.0",
    "drizzle-orm": "^0.27.2"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.6.7"
}
